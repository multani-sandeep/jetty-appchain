	<!-- Commit Booking -->
	<route url="/agentdesktop/commitAmendBooking">
		<node name="Hybris-AD">
			<step seq="1">
				<http url="http://localhost:8282/commercial-cpm/v1/bookings">
					<payload>Empty</payload>
