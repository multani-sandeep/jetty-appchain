<routes>
	<route url="/agentdesktop/login">
		<node name="Hybris-AD">
			<step seq="1">
				<http url="http://localhost:8282/commercial-cpm/agent-login-request">
					<payload>Empty</payload>
				</http>
			</step>
		</node>
	</route>
	<route url="/commercial-cpm/agent-login-request" >
		<node name="Hybris-ACP">
			<step seq="1">
				<serve>
					<payload>Agent Login Successful</payload>
				</serve>
			</step>
		</node>
	</route>
	<route url="/" default="true" >
		<node >
			<step seq="1">
				<http url="http://localhost:8282/commercial-cpm/agent-login-request">
					<payload>Empty</payload>
				</http>
			</step>
		</node>
	</route>
</routes>